        <PAGEOBJECT XPOS="981.88942992126" YPOS="4566.94595026837" OwnPage="14" ItemID="461850660" PTYPE="4" WIDTH="359.684825990985" HEIGHT="455.267747369348" FRTYPE="0" CLIPEDIT="1" PWIDTH="0.999999406761276" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L359.685 0 L359.685 455.268 L0 455.268 L0 0 Z" copath="M0 0 L359.686 0 L359.686 455.269 L0 455.269 L0 0 Z" gXpos="2.8421709430404e-14" gYpos="60.0062992125941" gWidth="740.629439370079" gHeight="515.274399999999" ALIGN="3" LAYER="2" NEXTITEM="-1" BACKITEM="-1">
