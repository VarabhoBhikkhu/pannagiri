        <PAGEOBJECT XPOS="140.00062992126" YPOS="9669.16261417323" OwnPage="29" ItemID="1520913333" PTYPE="4" WIDTH="360.025196850394" HEIGHT="435.266840944882" FRTYPE="0" CLIPEDIT="1" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L360.025 0 L360.025 435.267 L0 435.267 L0 0 Z" copath="M0 0 L360.026 0 L360.026 435.267 L0 435.267 L0 0 Z" gXpos="140.00062992126" gYpos="9669.16261417323" gWidth="360.025196850394" gHeight="451.772025196848" ALIGN="3" LAYER="3" NEXTITEM="-1" BACKITEM="-1">
