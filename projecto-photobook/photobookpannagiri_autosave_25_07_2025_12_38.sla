        <PAGEOBJECT XPOS="1362.83382992126" YPOS="7088.05241574803" OwnPage="22" ItemID="1084489567" PTYPE="4" WIDTH="380.9444" HEIGHT="475.269360629921" FRTYPE="0" CLIPEDIT="1" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="0" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L380.944 0 L380.944 475.269 L0 475.269 L0 0 Z" copath="M0 0 L380.944 0 L380.944 475.27 L0 475.27 L0 0 Z" gXpos="324.230744206974" gYpos="1240.57254359511" gWidth="500" gHeight="413.149171428571" ALIGN="3" LAYER="2" NEXTITEM="-1" BACKITEM="-1">
